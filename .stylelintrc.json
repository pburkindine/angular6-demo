{
  "extends": [
    "stylelint-config-sass-guidelines",
    "stylelint-config-prettier"
  ],
  "defaultSeverity": "error",
  "rules": {
    "font-weight-notation": ["numeric"],
    "selector-class-pattern": ["[A-Za-z_-]*"],
    "selector-max-compound-selectors": 4,
    "max-nesting-depth": 2
  }
}
